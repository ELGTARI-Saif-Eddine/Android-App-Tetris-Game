        android:endColor="#e601ff"
        android:centerColor="#e958fa"
        android:startColor="#e601ff"
        android:type="linear" />

    <!--If your shape requires only one solid color-->
    <!--<solid
        android:color="#FFFFFF" />-->

    <!--
    <size
        android:width="82dp"
